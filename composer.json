{
    "name": "vormkracht10/package-template",
    "description": ":package_description",
    "keywords": [
        "vormkracht10",
        "package-template"
    ],
    "homepage": "https://github.com/vormkracht10/package-template",
    "license": "MIT",
    "authors": [
        {
            "name": "Vormkracht10",
            "email": "info@vormkracht10.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "orchestra/testbench": "^6.12",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.10",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Skeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Skeleton\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vormkracht10\\PackageTemplate\\PackageTemplateProvider"
            ],
            "aliases": {
                "PackageTemplate": "Vormkracht10\\PackageTemplate\\Facades\\PackageTemplate"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
